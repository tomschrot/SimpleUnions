<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>

    <TargetFramework>net10.0</TargetFramework>
    <LangVersion>preview</LangVersion>
    <EnablePreviewFeatures>True</EnablePreviewFeatures>

    <RootNamespace>Test_10</RootNamespace>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>

    <NoWarn>$(NoWarn),IDE0044, IDE1006, IDE0060, IDE0075, CS0414</NoWarn>

    <Version>0.1.0-Beta</Version>
    <AssemblyVersion>0.1.0</AssemblyVersion>

  </PropertyGroup>

  <ItemGroup>
    <Using Include="System"></Using>
    <Using Include="System.IO"></Using>

    <Using Include="System.Text"></Using>
    <Using Include="System.Text.Json"></Using>

    <!-- If there is a conflict, remove a namespace! -->
    <Using Remove="System.Reflection"></Using>

    <Using Include="System.Collections.Generic"></Using>

    <!-- You can also have static usings (see Program Writeline) -->
    <Using Include="System.Console" Static="True"></Using>
  </ItemGroup>


</Project>
